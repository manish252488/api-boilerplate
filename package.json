{
  "name": "attachimp-api",
  "description": "A NodeJS server boilerplate for help you kickstart you futur node project. ES6/ES7 features.",
  "version": "1.0.3",
  "author": "Emanuel Quimper <quimperemanuel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EQuimper/nodejs-api-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/EQuimper/nodejs-api-boilerplate/issues"
  },
  "engines": {
    "node": ">= 6.10",
    "npm": ">= 3.10",
    "yarn": ">=0.20.3"
  },
  "main": "./src/index.js",
  "keywords": [
    "express",
    "es6",
    "es7",
    "rest",
    "api",
    "boilerplate",
    "mongodb",
    "node",
    "javascript",
    "pm2",
    "babel",
    "nps",
    "commitizen",
    "semantic-release",
    "prettier",
    "lint-staged",
    "webpack2"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "scripts": {
    "build": "nps build",
    "start": "nps",
    "test": "nps test",
    "test:watch": "nps test.watch",
    "dev": "npm rebuild @tensorflow/tfjs-node --build-addon-from-source && nps dev",
    "dev:debug": "nps dev.debug",
    "prettier": "prettier --write '{src,__{tests,mocks}__}/**/*.js'",
    "checkCover": "nps test.checkCover",
    "cover": "nps test.cover",
    "db:seeds-user": "nps db.seedsUser",
    "db:seeds-clear": "nps db.seedsClearUser",
    "db:seeds-clear-user": "nps db.seedsClear",
    "lint": "nps lint",
    "lint:fix": "nps lint.fix",
    "reportCoverage": "nps reportCoverage",
    "doc": "nps doc",
    "doc:deploy": "nps doc.deploy",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
